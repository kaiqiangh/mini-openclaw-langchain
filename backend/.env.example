# DeepSeek/OpenAI-compatible chat model configuration
DEEPSEEK_API_KEY=sk-xxx
DEEPSEEK_BASE_URL=https://api.deepseek.com
DEEPSEEK_MODEL=deepseek-chat

# Embedding model configuration
EMBEDDING_PROVIDER=openai
OPENAI_API_KEY=sk-xxx
OPENAI_BASE_URL=https://api.openai.com/v1
EMBEDDING_MODEL=text-embedding-3-small

# Google AI Studio embedding configuration (used when EMBEDDING_PROVIDER=google_ai_studio)
GOOGLE_API_KEY=
GOOGLE_EMBEDDING_MODEL=gemini-embedding-001

# App runtime
APP_HOST=0.0.0.0
APP_PORT=8002
APP_ENV=development
APP_ALLOWED_ORIGINS=http://localhost:3000,http://127.0.0.1:3000
APP_TRUSTED_HOSTS=localhost,127.0.0.1,*.localhost

# Optional tracing toggles (LangSmith only)
# Prefer LANGSMITH_TRACING as the main switch.
LANGSMITH_TRACING=false
# Global override across tracing toggles (optional)
OBS_TRACING_ENABLED=
LANGSMITH_ENDPOINT=https://api.smith.langchain.com
LANGSMITH_API_KEY=
LANGSMITH_PROJECT="mini-openclaw"
